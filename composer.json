{
    "name": "setono/client-id-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony bundle that allows you to track your visitors server side instead of client side",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.4",
        "setono/client-id": "dev-master",
        "setono/client-id-contracts": "dev-master",
        "symfony/config": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^4.4 || ^5.0",
        "symfony/event-dispatcher": "^4.4 || ^5.0",
        "symfony/http-foundation": "^4.4 || ^5.0.7",
        "symfony/http-kernel": "^4.4 || ^5.1.5",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "nyholm/symfony-bundle-test": "^1.7",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-symfony": "^2.1",
        "roave/security-advisories": "dev-master",
        "setono/code-quality-pack": "^2.0.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\ClientIdBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\ClientIdBundle\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpspec": "phpspec run",
        "phpunit": "phpunit"
    }
}
